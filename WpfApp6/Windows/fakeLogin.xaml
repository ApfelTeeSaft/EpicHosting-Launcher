<Window x:Class="Launcher.Windows.fakeLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher.Windows"
        mc:Ignorable="d"
        Title="fakeLogin" Height="450" Width="800" Background="Black">
    <Grid>
        <Label Content="Email:" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Foreground="Aqua"/>
        <Label Content="Password:" HorizontalAlignment="Left" Margin="10,111,0,0" VerticalAlignment="Top" Foreground="Aqua"/>
        <TextBox x:Name="EmailBox" Margin="0,38,0,0" TextWrapping="Wrap" Text="" PreviewTextInput="TextBox_PreviewTextInput" VerticalAlignment="Top" Width="352" TextChanged="TextBox_TextChanged"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Center" Margin="0,104,0,0" VerticalAlignment="Top" Width="352"/>
    </Grid>
</Window>
